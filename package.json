{
    "name": "whitenoise",
    "version": "0.1.0",
    "description": "",
    "type": "module",
    "scripts": {
        "clean": "rm -rf .svelte-kit node_modules/.vite node_modules/.cache/esbuild && bun install --force",
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "biome": "biome check .",
        "test": "svelte-kit sync && bunx vitest run --coverage && cd src-tauri && cargo test --tests",
        "check-migrations": "bash check_migrations.sh",
        "clippy": "cd src-tauri && cargo clippy --no-deps --workspace --all-features --all-targets -- -D warnings",
        "rustfmt": "cd src-tauri && cargo fmt -- --check",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "tauri": "tauri",
        "check-all": "bun run check-migrations && bun run biome && bun run test && bun run clippy && bun run rustfmt",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "license": "MIT",
    "dependencies": {
        "@tauri-apps/api": "^2.5.0",
        "@tauri-apps/plugin-barcode-scanner": "~2.2.0",
        "@tauri-apps/plugin-clipboard-manager": "^2.2.2",
        "@tauri-apps/plugin-dialog": "~2.2.1",
        "@tauri-apps/plugin-fs": "~2.2.1",
        "@tauri-apps/plugin-notification": "^2.2.2",
        "@tauri-apps/plugin-shell": "^2.2.1",
        "@types/qrcode": "^1.5.5",
        "carbon-icons-svelte": "^13.3.0",
        "mode-watcher": "^1.0.2",
        "nostr-tools": "^2.12.0",
        "paneforge": "^0.0.6",
        "qrcode": "^1.5.4",
        "svelte-gestures": "^5.1.4",
        "svelte-i18n": "^4.0.1",
        "svelte-sonner": "^0.3.28"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@storybook/addon-essentials": "8.6.12",
        "@storybook/addon-svelte-csf": "^5.0.0-next.29",
        "@storybook/addon-themes": "8.6.12",
        "@storybook/blocks": "8.6.12",
        "@storybook/svelte": "8.6.12",
        "@storybook/sveltekit": "8.6.12",
        "@storybook/test": "8.6.12",
        "@storybook/types": "^8.6.12",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/kit": "^2.20.7",
        "@sveltejs/vite-plugin-svelte": "^5.0.3",
        "@tailwindcss/forms": "^0.5.10",
        "@tauri-apps/cli": "^2.5.0",
        "@vitest/coverage-v8": "3.1.2",
        "autoprefixer": "^10.4.21",
        "bits-ui": "0.22.0",
        "clsx": "^2.1.1",
        "jsdom": "^26.1.0",
        "postcss": "^8.5.3",
        "storybook": "8.6.12",
        "svelte": "^5.28.2",
        "svelte-check": "^4.1.6",
        "tailwind-merge": "^3.2.0",
        "tailwind-variants": "^1.0.0",
